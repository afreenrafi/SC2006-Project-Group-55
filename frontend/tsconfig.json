{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "cultivate/index.jsx",
    "cultivate/_layout.jsx",
    "app-example/_layout.jsx",
    "app-example/+html.jsx",
    "app-example/+not-found.jsx",
    "components/Collapsible.jsx",
    "components/ExternalLink.jsx",
    "app-example/(tabs)/_layout.jsx",
    "app-example/(tabs)/index.jsx",
    "app-example/(tabs)/explore.jsx",
    "components/HelloWave.jsx",
    "components/ParallaxScrollView.jsx",
    "components/ThemedText.jsx",
    "components/ThemedView.jsx",
    "constants/Colors.js",
    "hooks/useColorScheme.jsx",
    "hooks/useColorScheme.web.js",
    "hooks/useThemeColor.js",
    "components/navigation/TabBarIcon.jsx"
  ]
}
